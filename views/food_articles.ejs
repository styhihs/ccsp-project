<!-- waterfall excerpts -->
<div class="container waterfall" data-col-min-width="240" data-default-container-width="600" data-autoresize="true">
	<% var len = items.length; %>
	<% for(var i = 0; i < len; i++) { %>
	<div data-toggle="modal" data-target="#article1">
		<div class="article-title"><%= items[i]['food'] %></div>
		<div class="article-content">
			時間：<%= items[i]['date'] %><br/>
			地點：<%= items[i]['location'] %><br/>
			理由：<%= items[i]['reason'] %><br/>
			編號：<%= items[i]['idNum'] %><br/>
			法條：<%= items[i]['law'] %><br/>
			懲處：<%= items[i]['punish'] %><br/>
		</div>
	</div>
	<% } %>
</div>